            <version>2.0.0-alpha-2</version>
        </dependency>
        <!-- Aether SPI -->
        <dependency>
            <groupId>org.apache.maven.resolver</groupId>
            <artifactId>maven-resolver-spi</artifactId>
            <version>2.0.0-alpha-2</version>
        </dependency>
        <!-- Aether Implementation -->
        <dependency>
