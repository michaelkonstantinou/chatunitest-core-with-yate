<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <!--gav information-->
    <groupId>com.mkonst</groupId>
    <artifactId>chatunitest-core-with-yate</artifactId>
    <!--Uploading to the SNAPSHOT repository, the version suffix must include 'SNAPSHOT'-->
    <version>1.0-SNAPSHOT</version>
    <packaging>jar</packaging>
    <name>chatunitest-core</name>
    <description>A framework to generate unit tests using LLMs</description>
    <url>https://github.com/ZJU-ACES-ISE/chatunitest-core</url>
    <licenses>
        <license>
            <name>MIT License</name>
            <url>https://www.opensource.org/licenses/mit-license.php</url>
        </license>
    </licenses>
    <scm>
        <connection>git@github.com:ZJU-ACES-ISE/chatunitest-core.git</connection>
        <url>https://github.com/ZJU-ACES-ISE/chatunitest-core</url>
    </scm>
    <developers>
        <developer>
            <name></name>
            <email></email>
        </developer>
    </developers>
    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <maven.compiler.source>17</maven.compiler.source>
        <maven.compiler.target>17</maven.compiler.target>
    </properties>
    <dependencies>
        <!-- Yate Core -->
        <dependency>
            <groupId>com.mkonst</groupId>
